<Window x:Class="Tours.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tours"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="159*"/>
            <RowDefinition Height="30*"/>


        </Grid.RowDefinitions>

        <Image Source="pack://siteoforigin:,,,/logo.png" HorizontalAlignment="Left" />
        <TextBlock Text=" Header" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Grid Background="#bae3e8" Panel.ZIndex="-2"/>
        <Grid Grid.Row="2" Background="#445c93"></Grid>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" Grid.Row="1" Name="MainFrame" Margin="-1,0,1,0"> </Frame>
        <Button Content="Back"  HorizontalAlignment="Right"  Click="ButtonBack_Click" Name="BtnBack"></Button>
        
    </Grid>
</Window>
